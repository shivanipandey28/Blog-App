<p>Welcome</p>
<h1>Articles</h1>
<ul>
  <% @articles.each do |article| %>
    <li>
      <%= link_to  article.title, article %>
    </li>
<% end %>
</ul>
<ul>
  <li><%= link_to "New Article ", new_article_path(@article)%>
</li>
  <% if user_signed_in? %>
  <%= link_to "Sign out",destroy_user_session_path, method: :delete%>
  <% else %>
  <%= link_to "Sign in", new_user_session_path  %>
  <% end %>
</ul>
